# Production Environment for Render
NODE_ENV=production
PORT=10000

# Database
DATABASE_URL=file:./data/production.db

# JWT Configuration
JWT_SECRET=your-super-secure-jwt-secret-key-change-this-in-render

# Security
CORS_ORIGIN=https://ai-consultancy-frontend.onrender.com
BCRYPT_ROUNDS=12

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# Monitoring
ENABLE_MONITORING=true
LOG_LEVEL=error

# API Configuration
API_PREFIX=api/v1
SWAGGER_ENABLED=false
