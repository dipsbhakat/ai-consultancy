# Production Environment Configuration
VITE_APP_ENV=production

# API Configuration
VITE_API_BASE_URL=https://ai-consultancy-backend-nodejs.onrender.com/api/v1
VITE_API_URL=https://ai-consultancy-backend-nodejs.onrender.com/api/v1
VITE_API_TIMEOUT=10000
VITE_API_RETRY_ATTEMPTS=3
VITE_API_RETRY_DELAY=1000

# Application Configuration
VITE_APP_TITLE=AI Consultancy - Production
VITE_APP_DESCRIPTION=Professional AI consulting services for your business
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_CHATBOT=true
VITE_ENABLE_LEAD_SCORING=true
VITE_ENABLE_PERSONALIZATION=true
VITE_ENABLE_AB_TESTING=true
VITE_ENABLE_ENTERPRISE_AI=true
VITE_ENABLE_BUSINESS_INTELLIGENCE=true
VITE_ENABLE_COMPETITIVE_INTELLIGENCE=true

# Security Configuration
VITE_ENABLE_CSP=true
VITE_ENABLE_SECURITY_HEADERS=true
VITE_JWT_SECRET_KEY=your-production-jwt-secret-here
VITE_CSRF_TOKEN_NAME=X-CSRF-Token

# Logging Configuration
VITE_LOG_LEVEL=error
VITE_ENABLE_CONSOLE_LOGS=false

# Cache Configuration
VITE_CACHE_DURATION=300000
VITE_ENABLE_CACHE=true

# Rate Limiting
VITE_RATE_LIMIT_PER_MINUTE=100

# Analytics & Monitoring (Update with your actual values)
VITE_SENTRY_DSN=your-sentry-dsn-for-frontend-monitoring
VITE_ANALYTICS_ID=your-google-analytics-id
