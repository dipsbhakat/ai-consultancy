{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-with-types": "tsc -b && vite build",
    "build:prod": "NODE_ENV=production vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview --host 0.0.0.0 --port ${PORT:-4173}",
    "start": "node server.js",
    "start:prod": "node server-prod.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:silent": "vitest --run --silent",
    "analyze": "npx vite-bundle-analyzer dist/stats.html",
    "deploy": "./deploy.sh",
    "deploy:prod": "./deploy.sh --deploy",
    "security:audit": "npm audit --audit-level high",
    "performance:analyze": "npx lighthouse http://localhost:4173 --output html --output-path ./lighthouse-report.html",
    "clean": "rm -rf dist node_modules/.vite",
    "postbuild": "echo 'Build completed successfully!' && ls -la dist/"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.8",
    "@hookform/resolvers": "^5.2.2",
    "@tailwindcss/forms": "^0.5.10",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "express": "^4.18.2",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.544.0",
    "node-fetch": "^2.7.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.9.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.14",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "eslint": "^9.33.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^27.0.0",
    "typescript-eslint": "^8.39.1",
    "vitest": "^3.2.4"
  }
}
